   </dir>
   <dir name="src" baseinstalldir="/">
    <dir name="UNL">
     <dir name="Geography">
      <dir name="SpatialData">
       <file role="php" name="Campus.php"/>
       <file role="php" name="DriverInterface.php"/>
       <file role="php" name="PDOSQLiteDriver.php"/>
       <file role="php" name="SQLiteDriver.php"/>
       <file role="php" name="UNLMapsWebServiceDriver.php"/>
      </dir>
     </dir>
    </dir>
   </dir>
  </dir>
 </contents>
